{
  "extends": ["google"],
  "rules": {
    // Additional, per-project rules...
    "one-var": "off",
    "require-jsdoc":"off",
    "semi": ["error", "always", { "omitLastInOneLineBlock": true }],
    "space-before-function-paren": ["error", "never"]
  },
  "parserOptions": {
    "ecmaVersion": 6
  }
}
